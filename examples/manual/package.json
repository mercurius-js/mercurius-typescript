{
  "name": "example-manual",
  "version": "1.0.0",
  "private": true,
  "description": "Mercurius basic example using only included types",
  "license": "MIT",
  "author": "PabloSz",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsup-node --watch --onSuccess \"cross-env NODE_ENV=development node dist/index.js\" src/index.ts",
    "prepare": "tsc",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "cross-env NODE_ENV=test tap --node-arg=--require=esbuild-register"
  },
  "dependencies": {
    "fastify": "^3.22.0",
    "graphql": "^15.6.1",
    "mercurius": "^8.5.0"
  },
  "devDependencies": {
    "@types/node": "^16.10.3",
    "@types/tap": "^15.0.5",
    "cross-env": "^7.0.3",
    "esbuild-register": "^3.0.0",
    "graphql-tag": "^2.12.5",
    "mercurius-integration-testing": "^3.2.0",
    "prettier": "^2.4.1",
    "tap": "^15.0.10",
    "tsup": "^5.3.0",
    "typescript": "^4.4.3"
  }
}
